<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vjtiapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/vjti_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/vjti_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Map"
            android:label="@string/map1" />
        <activity
            android:name=".Event"
            android:label="@string/events1" />
        <activity
            android:name=".Homescreen"
            android:label="@string/vjti_home" />
        <activity
            android:name=".Profile"
            android:label="@string/profile1" />
        <activity android:name=".ThankYou" />
        <activity android:name=".MainActivity">

        </activity>
    </application>

</manifest>